<div id="accordion">
<div>
<h3>Managed Node Information</h3>
<div class="net_content">
              <table class="net_info">
              <tbody>
                <tr>
                  <td>Platform</td>
                  <td class="sub_net_info">{% set ansible_network_os = hostvars[managed_node]['ansible_net_system'] %}{{ vendor[ansible_network_os]}} {{hostvars[managed_node]['ansible_net_system'] }}</td>
                </tr>
                <tr>
                  <td>Code Version</td>
                  <td class="sub_net_info">{{ hostvars[managed_node]['ansible_net_version'] }}</td>
                </tr>
                <tr>
                  <td>Model</td>
                  <td class="sub_net_info">{{ hostvars[managed_node]['ansible_net_model'] }}</td>
                </tr>
                <tr>
                  <td>Serial Number</td>
                  <td class="sub_net_info">{{ hostvars[managed_node]['ansible_net_serialnum'] }}</td>
                </tr>
              </tbody>
              </table>
</div>
</div>
</div>